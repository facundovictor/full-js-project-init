{
  "name": "full-js-project-init",
  "version": "0.0.1",
  "description": "A JS web project",
  "main": "api/app.js",
  "dependencies": {
    "express": "^4.14.0",
    "moment": "^2.17.1",
    "mysql": "^2.12.0",
    "sequelize": "^3.29.0",
    "swagger-express-mw": "^0.7.0",
    "umzug": "^1.11.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "commander": "^2.9.0",
    "gulp": "^3.9.1",
    "gulp-angular-protractor": "^0.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect-multi": "^1.0.8",
    "gulp-cssnano": "^2.1.2",
    "gulp-postcss": "^6.3.0",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^2.0.0",
    "gulp-watch": "^4.3.11",
    "jasmine-spec-reporter": "^3.2.0",
    "should": "^11.1.2",
    "supertest": "^2.0.1",
    "swagger": "^0.7.5",
    "swagger-editor": "^2.10.4",
    "uglify-js": "git+https://github.com/mishoo/UglifyJS2.git#harmony"
  },
  "scripts": {
    "server": "node ./api/app.js",
    "mock-server": "NODE_CONFIG_DIR=./api/config/ ./node_modules/swagger/bin/swagger.js project start -m ./api/",
    "debug": "node debug ./api/app.js",
    "migration": "node ./database/migration.js",
    "edit": "swagger_swagger_editorDir=node_modules/swagger-editor ./node_modules/swagger/bin/swagger.js project edit",
    "test-integration": "INTEGRATION=true NODE_CONFIG_DIR=./api/config/ ./node_modules/swagger/bin/swagger.js project test ./api/",
    "test": "NODE_CONFIG_DIR=./api/config/ ./node_modules/swagger/bin/swagger.js project test -m ./api/",

    "build": "./node_modules/gulp/bin/gulp.js --gulpfile ./client/gulpfile.js build",
    "dev": "./node_modules/gulp/bin/gulp.js --gulpfile ./client/gulpfile.js development",
    "build-and-test": "./node_modules/gulp/bin/gulp.js --gulpfile ./client/gulpfile.js test",
    "test-client": "./node_modules/gulp/bin/gulp.js --gulpfile ./client/gulpfile.js protractor"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/facundovictor/full-js-project-init.git"
